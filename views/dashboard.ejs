<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta author="OGMD" date="28/08/22">
	<link rel="stylesheet" href="./css/style.css">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css" integrity="sha384-zCbKRCUGaJDkqS1kPbPd7TveP5iyJE0EjAuZQTgFLD2ylzuqKfdKlfG/eSrtxUkn" crossorigin="anonymous">
	<link rel="shortcut icon" href="/img/Hoja.svg" type="image/x-icon">
	<title>AFS | Sensor</title>
</head>

<body>
	<!--conteiner navbar-->
	<nav class="navbar navbar-light navbar-expand-lg" style="background-color:#a9e159; ">
		<img src="./img/HojaR.svg" width="50" height="50" class="d-inline-block align-top" alt="">
		<a class="navbar-brand" href="./echo.html">AFS</a>
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
			aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
	</nav>
	<!--end navbar-->

	<!--contenedor de wave-->
	<div class="conteiner-wave">
		<!-- Generated by https://smooth.ie/blogs/news/svg-wavey-transitions-between-sections -->
		<div class="wave" style="height: 150px; overflow: hidden;">
			<svg viewBox="0 0 500 150" preserveAspectRatio="none" style="height: 100%; width: 100%;">
				<path d="M0.00,49.98 C149.99,150.00 349.20,-49.98 500.00,49.98 L500.00,150.00 L0.00,150.00 Z"
					style="stroke: none; fill: #fff;"></path>
			</svg>
		</div>
	</div>
	<!--end conteiner wave-->

	<main class="main">
		<div class="container-flex">
			<!--Niveles de luz en lux-->
			<div class="sensor1 sens card">
				<img src="./img/DronLogo.png" class="card-img-top" alt="...">
				<div class="sensName1 card-title" id="sensName1">Lux</div>
				<div class="sensData1 sensData card-text" id="sensData1">Sens1Data</div>
			</div>
			<!--Niveles de luz en luz visible-->
			<div class="sensor2 sens card">
				<img src="./img/DronLogo.png" class="card-img-top" alt="...">
				<div class="sensName2 card-title" id="sensName1">Luz Visible</div>
				<div class="sensData2 sensData card-text" id="sensData2">Sens2Data</div>
			</div>
			<!--Niveles de luz en IR-->
			<div class="sensor3 sens card">
				<img src="./img/DronLogo.png" class="card-img-top" alt="...">
				<div class="sensName3 card-title" id="sensName1">IR</div>
				<div class="sensData3 sensData card-text" id="sensData3">Sens3Data</div>
			</div>
	    </div>

		<!--<div class="consulta">
			<div class="nombre">
				<h2 id="nombre">Nombre: </h2><p id="name">nombre</p>
				<h2 id="valor">Valor: </h2><p id="value">valor</p>
				<h2 id="N">Nitrogeno: </h2><p id="nitrogeno">N</p>
				<h2 id="P">Fosforo: </h2><p id="fosforo">P</p>
				<h2 id="k">Potasio: </h2><p id="potasio">K</p>
				<h2 id="Mg">Magnesio: </h2><p id="magnesio">Mg</p>
			</div>
		</div>-->
		<div class="consulta">
			<% if (results.length > 0) { %>
				<% results.forEach(function(row) { %>
					<p>Planta: <%= row.NamePlant %></p>
					<p>Valor: <%= row.Valor %></p>
					<p>Nitrogeno: <%= row.Nnitrogeno %></p>
					<p>Fosforo: <%= row.Pfosforo %></p>
					<p>Potasio: <%= row.Kpotasio %></p>
					<p>Magnesio: <%= row.MgMagnesio %></p>
					<hr>
				<% }); %>
			<% } else { %>
				<p>No hay datos disponibles</p>
			<% } %>
		</div>
		<!--
		<div class="map">
			<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d28782.13816600592!2d-103.41003248916013!3d25.612648599999986!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x868fd12c7fd02ab1%3A0x419a63853144a5d2!2sInstituto%20Tecnol%C3%B3gico%20de%20Torre%C3%B3n!5e0!3m2!1ses-419!2smx!4v1684007224454!5m2!1ses-419!2smx" 
			width="70%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
		</div>
		-->
		<div class="container-flex">
			<div class="sens card">
				<img src="" class="card-img-top apiimg" alt="...">
			</div>
		</div>
	</main>

	<!--footer-->
	<footer id="footer2">
		<div class="container">
			<div class="copyright">&copy; Copyright <strong><span>AFS</span></strong>. All Rights Reserved</div>
		</div>
	</footer>
	<!--end footer-->

	<!--<ul id="messages"></ul>-->

</body>
<script src="/socket.io/socket.io.js"></script>
<script src="./js/echo.js"></script>
<script src="../database/consulta.js"></script>
</html>